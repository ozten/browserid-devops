{
  "env": "production",
  "public_url": "https://webhead.intcluster.mozilla.com",
  "public_static_url": "https://static.webhead.intcluster.mozilla.com",
  "bind_to": {
    "host": "127.0.0.1"
  },


  "use_minified_resources": true,

  "smtp": {
    "host": "127.0.0.1"
  },
  "statsd": {
    "enabled": true
  },

"var_path": "/home/browserid/browserid/var/",

  "bcrypt_work_factor": 12,
  "max_compute_processes": null,
  "max_compute_duration": 10,
  "disable_primary_support": false,
  "enable_code_version": false,
  "default_lang": "en-US",
  "express_log_format": "default",

  "email_to_console": false,
  "keysigner": { "bind_to": { "port": 62700 } },
  "browserid": { "bind_to": { "port": 62700 } },
  "router": { "bind_to": { "port": 63300 } },
  "verifier": { "bind_to": { "port": 62800 } },
  "dbwriter": { "bind_to": { "port": 62700 } },
  "verifier_url": "https://browserid.org",
  "static_url": "https://static.webhead.intcluster.mozilla.com",
  "browserid_url": "http://127.0.0.1:62700",
  "keysigner_url": "http://keysign.intcluster.mozilla.com",
  "dbwriter_url": "http://dbwriter.intcluster.mozilla.com"
}
